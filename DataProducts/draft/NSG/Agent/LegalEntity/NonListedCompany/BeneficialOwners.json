{
  "openapi": "3.0.2",
  "info": {
    "title": "Non-listed Company Beneficial Owners",
    "description": "The list of beneficial owners of a non-listed company. The list of shareholders that exceed 25 % ownership in the company.",
    "version": "1.0.0"
  },
  "paths": {
    "/draft/NSG/Agent/LegalEntity/NonListedCompany/BeneficialOwners": {
      "post": {
        "summary": "Non-listed Company Beneficial Owners",
        "description": "Non-listed Company Beneficial Owners",
        "operationId": "request_draft_NSG_Agent_LegalEntity_NonListedCompany_BeneficialOwners",
        "parameters": [
          {
            "description": "Consent token",
            "required": true,
            "schema": {
              "title": "X-Consent-Token",
              "type": "string",
              "description": "Consent token"
            },
            "name": "x-consent-token",
            "in": "header"
          },
          {
            "description": "The login token. Value should be \"Bearer [token]\"",
            "required": true,
            "schema": {
              "title": "Authorization",
              "type": "string",
              "description": "The login token. Value should be \"Bearer [token]\""
            },
            "name": "authorization",
            "in": "header"
          },
          {
            "description": "The bare domain of the system that provided the token.",
            "required": false,
            "schema": {
              "title": "X-Authorization-Provider",
              "type": "string",
              "description": "The bare domain of the system that provided the token."
            },
            "name": "x-authorization-provider",
            "in": "header"
          }
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/BeneficialOwnersRequest"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/BeneficialOwnersResponse"
                }
              }
            }
          },
          "422": {
            "description": "Validation Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HTTPValidationError"
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "BeneficialOwnersRequest": {
        "title": "BeneficialOwnersRequest",
        "required": ["nationalIdentifier"],
        "type": "object",
        "properties": {
          "nationalIdentifier": {
            "title": "National identifier",
            "maxLength": 40,
            "type": "string",
            "description": "The national identifier of the non-listed company issued by the trade register in any Nordic country.",
            "example": "FIN: 2464491-9 / SWE: 5560125791 / NOR:  923609016"
          }
        }
      },
      "BeneficialOwnersResponse": {
        "title": "BeneficialOwnersResponse",
        "required": ["shareSeries", "shareholders"],
        "type": "object",
        "properties": {
          "shareSeries": {
            "title": "Share series",
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/ShareSeries"
            },
            "description": "The details of the share series of the company"
          },
          "shareholders": {
            "title": "Shareholders",
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/Shareholders"
            },
            "description": "The list of beneficial owners of the company"
          }
        }
      },
      "HTTPValidationError": {
        "title": "HTTPValidationError",
        "type": "object",
        "properties": {
          "detail": {
            "title": "Detail",
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/ValidationError"
            }
          }
        }
      },
      "Ownerships": {
        "title": "Ownerships",
        "required": ["shareSeriesClass", "quantity"],
        "type": "object",
        "properties": {
          "shareSeriesClass": {
            "title": "Share series class",
            "allOf": [
              {
                "$ref": "#/components/schemas/ShareSeriesClass"
              }
            ],
            "description": "The class of the share series that the shareholder owns",
            "example": "B"
          },
          "quantity": {
            "title": "Quantity",
            "type": "integer",
            "description": "The number of shares that the shareholder owns a share series",
            "example": 20
          }
        }
      },
      "ShareSeries": {
        "title": "ShareSeries",
        "required": ["shareSeriesClass", "numberOfShares", "votesPerShare"],
        "type": "object",
        "properties": {
          "shareSeriesClass": {
            "title": "Share series class",
            "allOf": [
              {
                "$ref": "#/components/schemas/ShareSeriesClass"
              }
            ],
            "description": "The type of the share series of a company",
            "example": "A"
          },
          "numberOfShares": {
            "title": "Number of shares",
            "type": "integer",
            "description": "The total number of the shares in the share series class",
            "example": 1000
          },
          "votesPerShare": {
            "title": "Votes per share",
            "type": "integer",
            "description": "The number of votes per share in the share series",
            "example": 1
          }
        }
      },
      "ShareSeriesClass": {
        "title": "ShareSeriesClass",
        "enum": ["A", "B", "C", "D", "E"],
        "type": "string",
        "description": "An enumeration."
      },
      "Shareholders": {
        "title": "Shareholders",
        "required": ["name", "ownerships"],
        "type": "object",
        "properties": {
          "name": {
            "title": "Name",
            "maxLength": 250,
            "type": "string",
            "description": "The name of a shareholder of the company",
            "example": "Lars Lindberg | Company Ltd"
          },
          "ownerships": {
            "title": "Ownerships",
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/Ownerships"
            },
            "description": "The list of ownerships that the shareholder has in the company"
          }
        }
      },
      "ValidationError": {
        "title": "ValidationError",
        "required": ["loc", "msg", "type"],
        "type": "object",
        "properties": {
          "loc": {
            "title": "Location",
            "type": "array",
            "items": {
              "anyOf": [
                {
                  "type": "string"
                },
                {
                  "type": "integer"
                }
              ]
            }
          },
          "msg": {
            "title": "Message",
            "type": "string"
          },
          "type": {
            "title": "Error Type",
            "type": "string"
          }
        }
      }
    }
  }
}
